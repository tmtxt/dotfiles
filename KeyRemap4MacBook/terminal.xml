<item>
  <item>
    <name>Option+M to Return</name>
    <identifier>private.terminal_optionM</identifier>
    <only>TERMINAL</only>
    <autogen>
      __KeyToKey__
      KeyCode::M,
      MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_OPTION,
      KeyCode::RETURN
    </autogen>
  </item>
  
  <item>
    <name>Option+U to Option+B</name>
    <identifier>private.terminal_optionU</identifier>
    <only>TERMINAL</only>
    <autogen>
      __KeyToKey__
      KeyCode::U,
      MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_OPTION,
      KeyCode::B,
      MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_OPTION
    </autogen>
  </item>

  <item>
    <name>Option+O to Option+F</name>
    <identifier>private.terminal_optionO</identifier>
    <only>TERMINAL</only>
    <autogen>
      __KeyToKey__
      KeyCode::O,
      MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_OPTION,
      KeyCode::F,
      MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_OPTION
    </autogen>
  </item>
  
  <item>
    <name>Option+F to Backward Delete</name>
    <identifier>private.terminal_optionF</identifier>
    <only>TERMINAL</only>
    <autogen>
      __KeyToKey__
      KeyCode::F,
      MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_OPTION | ModifierFlag::NONE,
      KeyCode::DELETE</autogen>
  </item>

  <item>
    <name>Option+Shift+F to Option+Backward Delete</name>
    <identifier>private.terminal_optionshiftF</identifier>
    <only>TERMINAL</only>
    <autogen>
      __KeyToKey__
      KeyCode::F,
      MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_OPTION | MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_SHIFT | ModifierFlag::NONE,
      KeyCode::DELETE,
      MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_OPTION
    </autogen>
  </item>

</item>
