  <!-- misc -->
  <item>
	  <name>Tmtxt Misc</name>
	  <item>
	    <name>Switch profile by Control-Shift-Fx</name>
	    <identifier>private.misc_controlshiftFx</identifier>
	    <autogen>
		    __KeyToKey__
		    KeyCode::F1, ModifierFlag::CONTROL_L | ModifierFlag::SHIFT_L,
		    KeyCode::VK_OPEN_URL_APP_KeyRemap4MacBook_Profile0
	    </autogen>
	    <autogen>
		    __KeyToKey__
		    KeyCode::F2, ModifierFlag::CONTROL_L | ModifierFlag::SHIFT_L,
		    KeyCode::VK_OPEN_URL_APP_KeyRemap4MacBook_Profile1
	    </autogen>
	  </item>

	  <item>
      <name>Disable Power Key</name>
      <appendix>(+ Fn+Power to Power)</appendix>
      <identifier>private.misc_droppower</identifier>
      <modifier_not>ModifierFlag::FN</modifier_not>
      <autogen>__ConsumerToKey__ ConsumerKeyCode::POWER, KeyCode::VK_NONE</autogen>
    </item>
	  <item>
      <name>Eject to F13</name>
      <appendix>(Fn+Eject to Eject)</appendix>
      <identifier>private.eject2f13</identifier>
      <autogen>__ConsumerToConsumer__ ConsumerKeyCode::EJECT, ModifierFlag::FN, ConsumerKeyCode::EJECT</autogen>
      <autogen>__ConsumerToKey__ ConsumerKeyCode::EJECT, KeyCode::F13</autogen>
    </item>
	  <item>
      <name>Command+= to Command+`</name>
      <identifier>private.command=</identifier>
      <autogen>
        __KeyToKey__
		    KeyCode::EQUAL,
        MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_COMMAND,
		    KeyCode::BACKQUOTE,
        MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_COMMAND
      </autogen>
    </item>
	  <item>
      <name>Shift+F9 to Volume Mute</name>
      <identifier>private.shiftf9</identifier>
      <autogen>
        __KeyToConsumer__
		    KeyCode::F9,
        MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_SHIFT,
		    ConsumerKeyCode::VOLUME_MUTE
      </autogen>
    </item>
	  <item>
      <name>Shift+F10 to Volume Down</name>
      <identifier>private.shiftf10</identifier>
      <autogen>
        __KeyToConsumer__
		    KeyCode::F10,
        MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_SHIFT,
		    ConsumerKeyCode::VOLUME_DOWN
      </autogen>
    </item>
	  <item>
      <name>Shift+F11 to Volume Up</name>
      <identifier>private.shiftf11</identifier>
      <autogen>
        __KeyToConsumer__
		    KeyCode::F11,
        MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_SHIFT,
		    ConsumerKeyCode::VOLUME_UP
      </autogen>
    </item>
  </item>
